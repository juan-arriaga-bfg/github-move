<view size="{270,190}">
    <border-view borderImage="Images/frame" edgeInsets="{8,8,8,8}" size="{100%,100%}" autoresizingMask="width height"/>
    <view layout="center vertical 10" frame="{{7,7},{@-14,@-14}}" id="background-view" autoresizingMask="width height">
        <tableview cells="dynamic" size="{@-10,100}" id="tableview" separatorStyle="none"
            scrollEnabled="NO" allowsSelection="NO" autoresizingMask="width">
            <tableviewcell id="cell" size="{100%,55}" reuseIdentifier="cell" autoresizingMask="height">
                <view id="cell-background" size="{100%,@-5}" layout="center horizontal 10" autoresizingMask="width height">
                    <imageview image="Images/error_icon" size="{22,22}" autoresizingMask="margin-top margin-bottom"/>
                    <htmllabel id="label" size="{@-45,@-8}" numberOfLines="0" autoresizingMask="width" lineBreakMode="word-wrap"/>
                </view>
            </tableviewcell>
        </tableview>
        <button size="{110,50}" title="Ok" onTap="okTapped" id="ok-button" autoresizingMask="margin-bottom"/>
    </view>
</view>
